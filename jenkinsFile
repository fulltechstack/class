pipeline {
    agent any
    stages {
        stage('Install') {
            steps {
            echo "Installing..."     
            bat 'npm install' 
            }
        }
        stage('test') {
            steps { 
            echo "Testing..."    
            bat 'npm run ng -- test --watch false' 
            }
        }
        stage('build') {
            steps { 
            echo "Building..."    
            bat 'npm run ng -- build' 
            }
        }
        stage('deploy') {
            steps { 
            echo "Deploying..."
            bat 'xcopy /s /y "C:/ProgramData/Jenkins/.jenkins/workspace/class/dist/class" "C:/Users/91999/Desktop/Test"'
         }
        }
       
    }
}

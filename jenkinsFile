pipeline {
    agent any
    stages {
        stage('Install') {
            steps { 
            bat 'npm install' 
            echo "Installing..."
            }
        }
        stage('build') {
            steps { 
            bat 'npm run ng -- build' 
            echo "Building..."
            }
        }
        stage('deploy') {
            steps { 
            bat "xcopy /s C:/ProgramData/Jenkins/.jenkins/workspace/class/dist/class C:/Users/91999/Desktop/Test"
            echo "Deploying..."
         }
        }
       
    }
}
